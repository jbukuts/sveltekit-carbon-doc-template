{
	"name": "svelte-vest-reborn",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently -k -n 'velite,next' -c 'green,blue' 'npm:velite:watch' 'vite dev'",
		"build": "npm run velite && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "eslint . --ext js,mjs,cjs,ts,tsx,svelte",
    "lint:md": "eslint ./content --ext md,mdx",
		"format": "prettier --write .",
		"velite": "npx velite",
		"velite:watch": "npx velite dev"
	},
	"devDependencies": {
		"@carbon/layout": "^11.21.0",
		"@carbon/themes": "^11.33.0",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-virtual": "^3.0.2",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/lodash": "^4.17.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"carbon-components": "^10.58.12",
		"carbon-components-svelte": "^0.84.0",
		"carbon-icons-svelte": "^12.6.0",
		"carbon-preprocess-svelte": "^0.10.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-mdx": "^3.1.5",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-svelte": "^2.35.1",
		"lodash": "^4.17.21",
		"mdsvex": "^0.11.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"remark-cli": "^12.0.0",
		"remark-frontmatter": "^5.0.0",
		"remark-lint": "^9.1.2",
		"remark-lint-first-heading-level": "^3.1.2",
		"remark-lint-frontmatter-schema": "^3.15.4",
		"remark-lint-list-item-spacing": "^4.1.2",
		"remark-lint-no-empty-url": "^3.1.2",
		"remark-lint-no-paragraph-content-indent": "^4.1.2",
		"remark-preset-lint-consistent": "^5.1.2",
		"remark-preset-lint-markdown-style-guide": "^5.1.3",
		"remark-preset-lint-recommended": "^6.1.3",
		"remark-preset-prettier": "^2.0.1",
		"rollup": "^4.13.0",
		"rollup-plugin-css-only": "^4.5.2",
		"rollup-plugin-svelte": "^7.2.0",
		"sass": "^1.72.0",
		"slugify": "^1.6.6",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unplugin-auto-import": "^0.17.5",
		"velite": "^0.1.0-beta.12",
		"vite": "^5.0.3",
		"vitest": "^1.2.0"
	},
	"type": "module"
}
